    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{C836E0C6-4F9C-4B5C-B5AD-0E123CD3EA5D}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;BEGIN TRAN&#xA;DELETE FROM DW&#xA;FROM &quot; + @[$Package::DestSchemaName] + &quot;.&quot; + @[$Package::DestTableName] + &quot; DW &#xA;INNER JOIN &quot;  +@[$Package::StageSchemaName] + &quot;.&quot; +@[$Package::StageTableName] +&quot;  ST&#xA;ON ST.SalesOrderDetailID=DW.SalesOrderDetailID&#xA;WHERE ST._EXECID = &quot; +((DT_WSTR, 16 )@[User::ExecutionID]) +&quot;&#xA;INSERT INTO &quot; + @[$Package::DestSchemaName] + &quot;.&quot; + @[$Package::DestTableName] + &quot;&#xA;(&quot;&#xA;+&#xA;@[$Package::DestinationTableColumns]+&quot;,_HASHROW,_EXECID,_LOAD_DATE,ID&quot;&#xA;+&#xA;&quot;&#xA;)&#xA;SELECT &quot;&#xA;+&#xA;@[$Package::DestinationTableColumns]+&quot;,_HASHROW,_EXECID,_LOAD_DATE,ID&quot;&#xA;&#xA;+&#xA;&quot;&#xA;&#xA;FROM &quot;&#xA; +@[$Package::StageSchemaName] + &quot;.&quot; +@[$Package::StageTableName] +&quot;&#xA;WHERE _EXECID = &quot; +((DT_WSTR, 16 )@[User::ExecutionID]) +&quot; AND _OPERATION &lt;&gt; 'D'&#xA;COMMIT TRAN&#xA;&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="SQL_FromStageToDW">
      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRAN
DELETE FROM DW
FROM DW.SalesOrderDetail DW 
INNER JOIN STAGE.SalesOrderDetail_Delta  ST
ON ST.SalesOrderDetailID=DW.SalesOrderDetailID
WHERE ST._EXECID = 0
INSERT INTO DW.SalesOrderDetail
([SalesOrderID], [SalesOrderDetailID], [CarrierTrackingNumber], [OrderQty], [ProductID], [SpecialOfferID], [UnitPrice], [UnitPriceDiscount], [LineTotal], [rowguid], [ModifiedDate],_HASHROW,_EXECID,_LOAD_DATE,ID
)
SELECT [SalesOrderID], [SalesOrderDetailID], [CarrierTrackingNumber], [OrderQty], [ProductID], [SpecialOfferID], [UnitPrice], [UnitPriceDiscount], [LineTotal], [rowguid], [ModifiedDate],_HASHROW,_EXECID,_LOAD_DATE,ID

FROM STAGE.SalesOrderDetail_Delta
WHERE _EXECID = 0 AND _OPERATION &lt;&gt; 'D'
COMMIT TRAN
</DTS:VariableValue>
