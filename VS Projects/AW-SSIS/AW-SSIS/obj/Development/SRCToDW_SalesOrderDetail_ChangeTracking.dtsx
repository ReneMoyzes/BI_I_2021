      DTS:Expression="&quot;&#x9;SELECT  &#xA;    CT.[SalesOrderDetailID],&#xA;&#x9;CT.[SalesOrderID],[CarrierTrackingNumber], [OrderQty], [ProductID], [SpecialOfferID], [UnitPrice], [UnitPriceDiscount], [LineTotal], [rowguid], [ModifiedDate],&#xA;    CT.SYS_CHANGE_OPERATION&#xA;FROM  &#xA;   [Sales].[SalesOrderDetail] AS P  &#xA;RIGHT OUTER JOIN  &#xA;    CHANGETABLE(CHANGES [Sales].[SalesOrderDetail], &quot;+ @[User::LastLoadedVersion]+&quot; ) AS CT&#xA;ON &#xA;    P.SalesOrderID = CT.SalesOrderID &#xA;&#x9;AND &#xA;&#x9;P.[SalesOrderDetailID]=CT.[SalesOrderDetailID]&quot;"
